busybox sleep 1
if [ -e /system/etc/init.d/X08VACUUM ]; then
busybox echo "...X08VACUUM Script = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;

