busybox clear
busybox echo
busybox echo -e "\e[01;33m     Spark...\e[00;37m"
busybox echo -e "\e[31m   //----------------------------------------//"
busybox echo -n "  "
busybox echo -e "\e[00;41m//       CHECKING MOD                    //\e[00;37;40m"
