busybox sleep 1
if [ -e /system/bin/nitro_off ]; then
busybox echo "...NITRO_OFF = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;

busybox echo ">>>...Checking for ADS_SYSTEM..."
