busybox sleep 1
if [ -e /system/etc/init.d/X07ZIP ]; then
busybox echo "...X07ZIP Script = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;

