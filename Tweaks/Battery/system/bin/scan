busybox sleep 1
if [ -e /system/xbin/busybox ]; then
busybox echo "...BUSYBOX = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;
   
busybox echo ">>>...Checking for BOOST..."
