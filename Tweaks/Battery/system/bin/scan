busybox sleep 1
if [ -e /system/etc/init.d/X05TCP ]; then
busybox echo "...X05TCP Script = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;

busybox echo ">>>...Checking for X06ADS..."
busybox sleep 1
if [ -e /system/etc/Ads_Nitro/X06ADS ]; then
busybox echo "...X06ADS Script = OK..."
busybox sleep 1
else
busybox echo "...NOT OK = ERROR..."
busybox sleep 1
fi;

